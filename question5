#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
 
 
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int no;
    scanf("%d",&no);
    int a[no];
    for(int i=0;i<no;i++)
    scanf("%d",&a[i]);
    for(int j=no-1;j>=0;j--)
    printf("%d ",a[j]);
 
    return 0;
}
